{
  "manifest_version": 3,
  "name": "PhishingDetector",
  "description": "Machine Learning based phishing site detection browser extension",
  "version": "1.0",
  "permissions": ["activeTab", "storage", "notifications"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
